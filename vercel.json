{
  "rewrites": [{ "source": "/(.*)", "destination": "/" }],
  "headers": [
    {
      "source": "/.well-known/nostr.json",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        },
	{
		"key": "Access-Control-Allow-Origin",
		"value": "*"
	}
      ]
    }
  ]
}
